    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-element-to-jsx-string": "^2.1.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "1.13.x",
    "webpack-dev-server": "1.14.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
